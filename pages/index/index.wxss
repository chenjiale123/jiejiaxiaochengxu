.iconList {
  background-color: #fff;

  width: 94%;
  box-sizing: border-box;
  height: 380rpx;
  display: flex;
  overflow: hidden;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 0 auto;

  border-radius: 20rpx;
  padding:20rpx;

position: relative;

}

.iconList>.item {
  width: 18%;
  height: auto;
  overflow: hidden;
  text-align: center;
}

.iconList>.item>image {
  width: 94%;
  height: 106rpx;
  display: block;
  margin: 0 auto;

}

.iconList>.item>text {
  display: block;
  width: 94%;
  font-size:22rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(51,51,51,1);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.goodList {
  border-radius: 20rpx;
  background-color: #fff;
  margin-top: 12rpx;
  width: 94%;
  height: auto;
  overflow: hidden;
  padding: 20rpx 0;
  box-sizing: border-box;
  margin: 0 auto;
  margin-top: 26rpx;
position: relative;

}

.goodList>.title {
  width: 100%;
  height: auto;
  overflow: hidden;
  display: flex;
  padding: 0 30rpx;
  justify-content: space-between;
  box-sizing: border-box;
}
.goodList>.title>.dian>image{
  width: 44rpx;
  height: 44rpx;
  vertical-align: middle;
}
.goodList>.title>.dian> text {
  font-size:30rpx;
  font-family:Microsoft YaHei;
  font-weight:bold;
  color:rgba(26,26,26,1);
  vertical-align: middle;
  margin-left: 8rpx;
}
.goodList>.title>view>image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.goodList>.title>view>text {
  font-size:24rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(255,76,79,1);
  vertical-align: middle;
}

.goodList>.list {
  margin-top: 28rpx;

  height: auto;
  overflow: hidden;
  display: flex;
  justify-content: flex-start;
  padding: 10rpx 10rpx;
  flex-wrap: wrap;
}

.goodList>.list>.item {
  width: calc(100%/3);
  height: auto;
  overflow: hidden;
  border-radius: 20rpx;
  padding: 10rpx;
  box-sizing: border-box;
}

.goodList>.list>.item>image {
  border-radius: 20rpx;
  width: 100%;
  height: 204rpx;
  display: block;
}
.goodList>.list>.item>.classIn{
  position: relative;
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 15rpx 0rpx 0 0rpx;

background:rgba(255,255,255,1);
/* border-radius:20rpx; */
/* top: -28rpx; */
box-sizing: border-box;
}

.goodList>.list>.item>.classIn>text {
  font-size:22rpx;
  font-family:Microsoft YaHei;
  font-weight:600;
  color:rgba(26,26,26,1);

}
.goodList>.list>.item>.classIn>view{
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;

}
.goodList>.list>.item>.classIn>view>view {
  font-size:24rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(255,76,79,1);
}
.goodList>.list>.item>.classIn>view>view>text{
  color:rgba(255,76,79,1);
  font-size:30rpx;
  font-weight: bold;
}

