/* pagesSinger//pages/viewWechat/viewWechat.wxss */

Page{
  width: 100%;
  height: 100%;
  background:rgba(242,245,247,1);
border-radius:30rpx 30rpx 0px 0px;
}
.contain{
  width: 100%;
  height: 100%;
  background:rgba(242,245,247,1);
border-radius:30rpx 30rpx 0px 0px;
overflow: hidden;
}
.tishi{
width: 164rpx;
height: auto;
overflow: hidden;
margin: 0 auto;
margin-top: 200rpx;
}
.tishi>text:nth-child(1){
  font-size:30rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(51,51,51,1);
display: block;
}
.tishi>text:nth-child(2){
  font-size:36rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(255,76,79,1);
}
.code{
  width:400rpx;
height:400rpx;
background:rgba(255,255,255,1);
border-radius:20rpx;
margin:0 auto;
margin-top: 50rpx;
padding: 28rpx;
box-sizing: border-box;
}
.code>image{
  width: 100%;
  height: 100%;
}
.phone{
  width: 405rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  text-align: center;
}
.phone>text:nth-child(1){
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  display: block;

}
.phone>text:nth-child(2){
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  display: block;
}